@model PersonViewModel;

@{
}

<div class="row" id="person">
    <p>@Model.FirstName</p>
    <p>@Model.LastName</p>
    <p>@Model.City.Name @Model.City.Country.Name</p>
    <p>@Model.PhoneNr</p>
    <p>@Model.SocialSecurityNr</p>
    @foreach (var language in @Model.Languages.Languages)
    {
        <p>@language.LanguageName</p>
    }

</div>