@model CountryViewModel
@{
}


<tr>
    <td>@Model.Name</td>
    <td>
        @foreach (CityViewModel city in @Model.Cities)
        {
            <p>@city.Name</p>
        }
    </td>
    <td><a asp-controller="Countries" asp-action="DeleteCountry" asp-route-id="@Model.Id">Remove</a></td>
</tr>



