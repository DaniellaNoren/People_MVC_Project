@model PeopleMVC.Data.Entities.ViewModels.PeopleViewModel;
@{
}
<table>
    @foreach (var p in Model.people)
    {
        <tr>
            <td>@p.FirstName</td>
            <td>@p.LastName</td>
            <td>@p.City</td>
            <td>@p.PhoneNr</td>
        </tr>
    }

</table>